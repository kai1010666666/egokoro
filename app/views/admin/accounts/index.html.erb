<div class="container">
  <div class="row">
    <div class="col">
      <h2>accounts</h2>
      <table>
          <thead>
            <tr>
              <th>image</th>
              <th>name</th>
              <th colspan="3"></th>
            </tr>
          </thead>
        <% @accounts.each do |account| %>
          <tbody>
            <tr>
              <td><%= image_tag account.get_profile_image(100,100) %></td>
              <td><%= account.name %></td>
              <td><%= link_to "Show", admin_account_path(account.id) %></td>
            </tr>
          </tbody>
        <% end %>
      </table>
    </div>
  </div>
</div>