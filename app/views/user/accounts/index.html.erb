<div class="container" >
  <div class="row">
    <div class="col">
      <h2>アカウント一覧</h2>
      <table>
        <% @accounts.each do |account| %>
          <tr>
            <td><%= image_tag account.get_profile_image(100,100) %></td>
            <td><%= account.name %></td>
            <td><%= link_to "アカウント詳細", account_path(account.id) %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>